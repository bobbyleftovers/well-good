{
  "name": "wellgood-2016",
  "version": "17.1.3",
  "scripts": {
    "create-module": "sh ../../../private/scripts/create_module.sh",
    "sync": "git fetch --all && git checkout master && git pull origin master && git checkout develop && git pull origin develop",
    "docker:start": "cd ../../../laradock && docker-compose up -d nginx mariadb workspace && cd ../wp-content/themes/wellgood-2016",
    "docker:stop": "cd ../../../laradock && docker-compose stop && cd ../wp-content/themes/wellgood-2016",
    "docker:reset": "npm run docker:stop && npm run docker:start",
    "dev": "npm run docker:start && npm run start",
    "start": "brrl -w -- url=https://well-good.lndo.site/",
    "start:main": "npm run start -- only=main",
    "start:summer": "npm run start -- only=summer",
    "start:all": "npm run start -- --all",
    "start:2020": "npm run start -- only=main-2020",
    "start:rich-tag": "npm run start -- only=rich-tag,main-2020",
    "start:rich-tag+vendor": "npm run start -- only=rich-tag,main-2020,vendor",
    "start:gutenberg": "npm run start -- only=gutenberg",
    "start:trends-2021": "npm run start -- only=trends-2021",
    "start:trends-2021+gutenberg": "npm run start -- only=trends-2021,gutenberg",
    "start:trends-2021+all": "npm run start -- only=trends-2021,gutenberg,vendor,main-2020",
    "start:renew-year": "npm run start -- only=renew-year-2021",
    "start:renew-year+gutenberg": "npm run start -- only=renew-year-2021,gutenberg",
    "start:changemakers": "npm run start -- only=changemakers",
    "start:styleguide": "npm run start -- only=main-2020,styleguide-2020",
    "start:critical": "npm run build && npm run start -- CRITICAL=true",
    "build": "brrl -b -e production --",
    "build:local": "npm run build -- local=true --",
    "build:gutenberg": "npm run build:local -- only=gutenberg",
    "build:vendor": "npm run build:local -- only=vendor",
    "critical": "npm run build -- CRITICAL=true --",
    "critical:develop": "npm run critical -- url=https://develop.stg.wellandgood.com/",
    "critical:local": "npm run critical -- url=https://well-good.lndo.site/",
    "dependencies": "brrl-install",
    "release:patch": "sh ../../../private/scripts/prepare.sh -v=patch",
    "release:minor": "sh ../../../private/scripts/prepare.sh -v=minor",
    "release:major": "sh ../../../private/scripts/prepare.sh -v=major",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
